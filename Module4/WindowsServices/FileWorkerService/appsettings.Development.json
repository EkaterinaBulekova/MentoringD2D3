{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "WorkerSettings": {
    "Directories": {
      "WatchedDirectories": [ "D:\\Images", "D:\\Images1" ],
      "TargetDirectory": "D:\\Out",
      "WatchTimeout": 10000
    },
    "FileProcessRule": {
      "Prefix": "image",
      "Delimeter": "_",
      "NumberFormat": "\\d{1,5}\\.",
      "Extentions": "png|jpeg|jpg",
      "Timeout": 20000
    }
  }
}
