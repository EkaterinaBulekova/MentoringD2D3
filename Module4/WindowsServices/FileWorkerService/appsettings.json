{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "WorkerSettings": {
    "Directories": {
      "WatchedDirectories": [ "D:\\Images", "D:\\Images1" ],
      "TargetDirectory": "D:\\Out",
      "WatchTimeout":  10000
    },
    "FileProcessRule": {
      "Prefix": "image",
      "Delimeter": "_",
      "NumberLenght": 5,
      "Extentions": "png|jpeg|jpg",
      "Timeout": 20000
    }
  }
}
